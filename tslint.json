{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "no-empty-interface": false,
    "no-console": false,
    "trailing-comma": false,
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "object-curly-spacing": [
      true,
      "always"
    ],
    "array-bracket-spacing": [
      true,
      "always"
    ],
    "no-unused-declaration": [
      true,
      "always"
    ],
    "no-unused-variable": [
      true,
      "check-parameters"
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
